{
  "Name": "No Derivative No Tissue Confounds No Scrubbing",
  "BIDSModelVersion": "1.0.0",
  "Description": "Model used for olfactory and frontal region. Combined GLM for olfaction identification and localization tasks - HRF: no derivatives - No Tissue Confounds - No Scrubbing - include all voxels in SPM ICV mask",
  "Input": {
    "task": [
      "olfid",
      "olfloc"
    ],
    "space": [
      "MNI152NLin2009cAsym"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "NoConfounds",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Transformations": {
        "Transformer": "cpp_spm",
        "Description": "add 250 ms to all olfactory onsets to take into account 'time of flight' delay for the odor stimulus to travel the tubing",
        "Instructions": [
          {
            "Name": "Add",
            "Input": [
              "onset"
            ],
            "Query": "trial_type==olf.*",
            "Value": 0.25
          }
        ]
      },      
      "Model": {
        "X": [
          "trial_type.olfid_eucalyptus_left",
          "trial_type.olfid_eucalyptus_right",
          "trial_type.olfid_almond_left",
          "trial_type.olfid_almond_right",
          "trial_type.olfloc_eucalyptus_left",
          "trial_type.olfloc_eucalyptus_right",
          "trial_type.olfloc_almond_left",
          "trial_type.olfloc_almond_right",
          "trial_type.resp_03",
          "trial_type.resp_12",
          "trans_?",
          "rot_?",
          1
        ],
        "HRF": {
          "Variables": [
            "trial_type.olfid_eucalyptus_left",
            "trial_type.olfid_eucalyptus_right",
            "trial_type.olfid_almond_left",
            "trial_type.olfid_almond_right",
            "trial_type.olfloc_eucalyptus_left",
            "trial_type.olfloc_eucalyptus_right",
            "trial_type.olfloc_almond_left",
            "trial_type.olfloc_almond_right",
            "trial_type.resp_03",
            "trial_type.resp_12"
          ],
          "Model": "spm"
        },
        "Type": "glm",
        "Options": {
          "HighPassFilterCutoffHz": 0.0078,
          "Mask": {
            "desc": [
              "brain"
            ],
            "suffix": [
              "mask"
            ]
          }
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST",
            "InclusiveMaskingThreshold": "-Inf"
          }
        }
      },
      "DummyContrasts": {
        "Contrasts": [
          "trial_type.olfid_eucalyptus_left",
          "trial_type.olfid_eucalyptus_right",
          "trial_type.olfid_almond_left",
          "trial_type.olfid_almond_right",
          "trial_type.olfloc_eucalyptus_left",
          "trial_type.olfloc_eucalyptus_right",
          "trial_type.olfloc_almond_left",
          "trial_type.olfloc_almond_right"
        ],
        "Test": "t"
      },
      "Contrasts": [
        {
          "Name": "Responses",
          "ConditionList": [
            "trial_type.resp_03",
            "trial_type.resp_12"
          ],
          "Weights": [
            1,
            1
          ],
          "Test": "t"
        },
        {
          "Name": "all_olfid",
          "ConditionList": [
            "trial_type.olfid_eucalyptus_left",
            "trial_type.olfid_eucalyptus_right",
            "trial_type.olfid_almond_left",
            "trial_type.olfid_almond_right"
          ],
          "Weights": [
            1,
            1,
            1,
            1
          ],
          "Test": "t"
        },
        {
          "Name": "all_olfloc",
          "ConditionList": [
